{
  "name": "arquivei/boltons-scaffolding",
  "type": "project",
  "license":"MIT",
  "description": "This project is used by our developers to speed up the coding of new use cases",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^7.1.3",
    "illuminate/console": "5.*"
  },
  "autoload": {
    "psr-4": {
      "Arquivei\\BoltonsScaffolding\\Core\\": "core/",
      "Arquivei\\BoltonsScaffolding\\": "."
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arquivei\\BoltonsScaffolding\\Laravel\\Providers\\LescriptProvider"
      ]
    }
  }
}
